{{define "rabbitmq"}}
<LoadPlugin "python">
{{- range $key, $value := .Vars }}
  {{$key}} {{$value}}
{{- end }}
</LoadPlugin>
<Plugin "python">
  ModulePath "{{Globals.PluginRoot}}/rabbitmq-collectd-plugin"
  Import "rabbitmq"
  {{range $instance := .Instances}}
    {{RenderPlugin $instance}}
  {{end}}
</Plugin>
{{end}}