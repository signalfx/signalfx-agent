{{define "elasticsearch"}}
<LoadPlugin "python">
  {{ range $key, $value := .Vars -}}
  {{$key}} {{$value}}
  {{ end -}}
</LoadPlugin>
<Plugin "python">
  ModulePath "{{Globals.PluginRoot}}/elasticsearch-collectd-plugin"
  Import "elasticsearch_collectd"
  {{range $instance := .Instances}}
    {{RenderPlugin $instance}}
  {{end}}
</Plugin>
{{end}}
