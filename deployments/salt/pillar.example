signalfx-agent:
  repo_base_url: https://dl.signalfx.com
  package_stage: final
  conf_file_path: '/etc/signalfx/agent.yaml'
  conf:
    signalFxAccessToken: 'My_Token'
    enableBuiltInFiltering: true
    monitors:
      - type: cpu
      - type: filesystems
      - type: disk-io
      - type: net-io
      - type: load
      - type: memory
      - type: vmem
      - type: processlist
