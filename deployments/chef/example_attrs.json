{"signalfx_agent": {
  "package_stage": "release",
  "agent_version": "4.3.1",
  "conf": {
    "signalFxAccessToken": "testing",
    "observers": [
      {"type": "host"}
    ], 
    "monitors": [
      {"type": "collectd/cpu"},
      {"type": "collectd/cpufreq"},
      {"type": "collectd/df"},
      {"type": "disk-io"},
      {"type": "collectd/interface"},
      {"type": "load"},
      {"type": "collectd/memory"},
      {"type": "collectd/signalfx-metadata", "omitProcessInfo": true},
      {"type": "collectd/vmem"},
      {"type": "host-metadata"}
      {"type": "processlist"}
    ],
    "enableBuiltInFiltering": true
  }
}}
