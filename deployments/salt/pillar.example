signalfx-agent:
  repo_base_url: https://dl.signalfx.com
  package_stage: final
  conf_file_path: '/etc/signalfx/agent.yaml'
  conf:
    signalFxAccessToken: 'My_Token'
    monitors:
      - type: collectd/cpu
      - type: collectd/cpufreq
      - type: collectd/df
      - type: collectd/disk
      - type: collectd/interface
      - type: collectd/load
      - type: collectd/memory
      - type: collectd/signalfx-metadata
      - type: collectd/vmem
