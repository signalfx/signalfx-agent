LoadPlugin "python"
<Plugin "python">
  ModulePath "{{Globals.PluginRoot}}/marathon"
  Import "marathon"
  LogTraces true
  <Module "marathon">
    host  ["{{.Host}}", "{{.Port}}", "{{.Username}}", "{{.Password}}"]
    verbose False
  </Module>
</Plugin>

<Chain "PostCache"> 
  <Rule "set_marathon_monitor_id"> 
    <Match "regex"> 
      Plugin "^marathon" 
    </Match> 
    <Target "set"> 
      MetaData "monitorID" "{{.MonitorID}}" 
    </Target> 
  </Rule> 
  Target "write" 
</Chain>
